{
  "name": "rulebound",
  "version": "1.1.1",
  "description": "A simple rule based testing framework",
  "license": "MIT",
  "main": "dist/rulebound.js",
  "module": "dist/rulebound.mjs",
  "types": "dist/rulebound.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -cw",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "nyc ts-mocha --spec test/unit/**/*",
    "test:integration": "ts-mocha --spec test/integration/**/*",
    "lint": "eslint ./*/**/*.{ts,js}",
    "prepublishOnly": "npm run lint && npm run test:unit -- --forbid-only && npm run test:integration -- --forbid-only && npm run build"
  },
  "author": {
    "name": "Sander van Beek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lakitna/Rulebound.git"
  },
  "bugs": {
    "url": "https://github.com/Lakitna/Rulebound/issues"
  },
  "keywords": [
    "rule",
    "rules",
    "test",
    "testing",
    "rule-based"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/is-glob": "^4.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/micromatch": "^4.0.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.5",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "choma": "^1.2.1",
    "eslint": "^8.26.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rollup": "^3.2.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-externals": "^5.0.1",
    "sinon": "^14.0.1",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cosmiconfig": "^7.0.1",
    "is-glob": "^4.0.3",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5"
  }
}
