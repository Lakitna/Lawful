{
  "name": "rulebound",
  "version": "1.1.0",
  "description": "A simple rule based testing framework",
  "license": "MIT",
  "main": "dist/rulebound.js",
  "module": "dist/rulebound.mjs",
  "types": "dist/rulebound.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -cw",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "nyc mocha test/unit",
    "test:integration": "mocha test/integration",
    "lint": "eslint ./*/**/*.{ts,js}",
    "prepublishOnly": "npm run lint && npm run test:unit -- --forbid-only && npm run test:integration -- --forbid-only && npm run build"
  },
  "author": {
    "name": "Sander van Beek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lakitna/Rulebound.git"
  },
  "bugs": {
    "url": "https://github.com/Lakitna/Rulebound/issues"
  },
  "keywords": [
    "rule",
    "rules",
    "test",
    "testing",
    "rule-based"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/is-glob": "^4.0.1",
    "@types/lodash": "^4.14.144",
    "@types/micromatch": "^3.1.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "choma": "^1.2.1",
    "eslint": "^6.6.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.2.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rollup": "^1.24.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-dts": "^1.1.10",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "cosmiconfig": "^6.0.0",
    "is-glob": "^4.0.1",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "winston": "^3.2.1"
  }
}
