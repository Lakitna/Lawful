{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "ordered-imports": false,
        "one-line": false,
        "no-consecutive-blank-lines": [true, 2],
        "variable-name": {
            "options": [
                "check-format",
                "allow-leading-underscore",
                "allow-pascal-case",
                "allow-snake-case",
                "ban-keywords"
            ]
        },
        "object-literal-sort-keys": false,
        "no-console": {
            "severity": "warning"
        }
    },
    "rulesDirectory": []
}